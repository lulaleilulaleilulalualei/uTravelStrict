.feeDetail {
  position: fixed;
  top:0;
  left: 0;
  z-index: 200;
  width: 100%;
  height: 100%;
  background-color: rgba(0,0,0,0.7);
  display: flex;
}
.feeDetalBox {
  flex: 1;
  width: 70%;
  height: auto;
  background: #F1F1F5;
  margin-top: 160rpx;
  padding-bottom: 100rpx;
}
.feeDetailTitle {
  width: 100%;
  height: 90rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  font-size: 36rpx;
  color: #222222;
  background: #fff;
}
.productFeeName {
  width: auto;
  height: 36rpx;
  padding: 20rpx 30rpx;
  display: flex;
  align-items: center;
  font-size: 24rpx;
  color: #000000;
}
.productFeeBox {
  padding: 0 30rpx;
  background: #fff;
}
.productDesc {
  font-size: 32rpx;
  color: #000000;
  line-height: 36rpx;
  padding: 20rpx 0;
  border-bottom: 1rpx solid #EFEFEF;
}
.productUserBuy {
  width: auto;
  padding: 20rpx 0;
  background-color: #fff;
}
.itemShowUserBuy {
  width: auto;
  display: flex;
  align-items: center;
  justify-content: space-between;
  font-size: 28rpx;
  color: #000000;
}
.itemShowUserBuy:not(:last-child) {
  margin-bottom: 14rpx;
}
.showNeedTotal {
  margin-top: 20rpx;
  padding: 0 30rpx;
  width: auto;
  height: 170rpx;
  display: flex;
  align-items: center;
  justify-content: flex-end;
  background-color: #fff;
}
.showNeedTotal>view {
  font-size: 14px;
  color: #000000;
  display: flex;
  align-items: center;
}
.showNeedTotal>view>text {
  display: block;
  font-size: 18px;
  color: #F7372C;
}
.toCloseFeeBox {
  position: absolute;
  bottom:0;
  left:0;
  width: 100%;
  height: 100rpx;
  background: #FFDA4C;
  display: flex;
  align-items: center;
  justify-content: center;
  font-size: 36rpx;
  color: #222222;
}


